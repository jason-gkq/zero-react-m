{
  "name": "zero-react-m",
  "version": "1.0.0",
  "description": "基于react和mobx构建的移动端h5网站",
  "private": true,
  "main": "./src/pages/index/index.tsx",
  "scripts": {
    "start": "szero-scripts start env=local",
    "build": "szero-scripts build env=prod",
    "build:test": "szero-scripts build env=test",
    "build:uat": "szero-scripts build env=uat",
    "build:prod": "szero-scripts build env=prod",
    "prepare": "husky install",
    "prettier:check": "prettier --check ./src",
    "prettier:write": "prettier --write ./src"
  },
  "repository": {
    "type": "git",
    "url": "git+https://github.com/jason-gkq/zero-react-m.git"
  },
  "keywords": [],
  "author": "guokeqin",
  "license": "ISC",
  "bugs": {
    "url": "https://github.com/jason-gkq/zero-react-m/issues"
  },
  "dependencies": {
    "@szero/mobile": "1.3.2",
    "antd-mobile": "5.28.0",
    "antd-mobile-icons": "0.3.0",
    "axios": "1.3.3",
    "core-js": "3.26.0",
    "crypto-js": "4.1.1",
    "docx-preview": "0.1.15",
    "exceljs": "^4.3.0",
    "mobx": "6.8.0",
    "mobx-react-lite": "3.4.0",
    "pdfjs-dist": "3.4.120",
    "prop-types": "15.8.1",
    "react": "18.2.0",
    "react-dom": "18.2.0",
    "react-router-dom": "6.8.1",
    "require": "2.4.20",
    "tinycolor2": "^1.6.0",
    "web-vitals": "3.1.1",
    "x-data-spreadsheet": "^1.1.9"
  },
  "browserslist": {
    "production": [
      "iOS >= 10",
      "Android >= 10",
      "Firefox > 65",
      "chrome > 72",
      "safari >= 10",
      ">0.2%",
      "not dead",
      "IE 11",
      "not IE 10",
      "last 2 versions",
      "not op_mini all"
    ],
    "development": [
      "last 2 chrome version",
      "last 2 firefox version",
      "last 2 safari version"
    ]
  },
  "devDependencies": {
    "@types/crypto-js": "4.1.1",
    "@types/exceljs": "^1.3.0",
    "@types/node": "18.14.0",
    "@types/react": "18.0.28",
    "@types/react-dom": "18.0.11",
    "@types/react-router-dom": "5.3.2",
    "@types/tinycolor2": "^1.4.3",
    "cross-spawn": "7.0.3",
    "husky": ">=6",
    "lint-staged": ">=10",
    "prettier": "2.8.4",
    "szero-scripts": "1.0.2"
  },
  "lint-staged": {
    "src/**/*.{ts,tsx,less,json,css,md}": "prettier --write --ignore-unknown"
  }
}
